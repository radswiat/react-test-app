    "build:live": "babel-node build-tools/npm-scripts/webpack-live.js --presets=env,stage-2",
    "start:tests": "jest --config test/unit/jest-config.js --no-cache -i",
    "start:tests:watch": "jest --config test/unit/jest-config.js -i --no-cache --watchAll",
    "start:e2e": "cypress open",
    "precommit": "lint-staged"
